{
  "name": "tradex",
  "version": "0.0.1",
  "description": "Crossplatform crypto trading platform (Mac / PC) ",
  "repository": {
    "type": "git",
    "url": "https://github.com/sakhnyuk/tradex.git"
  },
  "main": "./src/dist/main.prod.js",
  "scripts": {
    "build": "concurrently \"yarn build-main\" \"yarn build-renderer\"",
    "build-main": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.main.prod.config.ts --colors",
    "build-renderer": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.renderer.prod.config.ts --colors",
    "start-renderer-dev": "webpack-dev-server --config ./webpack/webpack.renderer.dev.config.ts",
    "start-main-dev": "webpack --config ./webpack/webpack.main.config.ts && electron ./src/dist/main.dev.js",
    "dev": "rimraf src/dist && cross-env START_HOT=1 NODE_ENV=development npm run start-renderer-dev",
    "package-all": "yarn build && electron-builder build -mwl",
    "package-mac": "yarn build && electron-builder build --mac",
    "package-linux": "yarn build && electron-builder build --linux",
    "package-win": "yarn build && electron-builder build --win --x64",
    "electron-rebuild": "electron-rebuild --parallel --force --types prod,dev,optional",
    "postinstall": "electron-builder install-app-deps"
  },
  "keywords": [
    "cryptocurrency",
    "terminal",
    "trading"
  ],
  "author": "Mikhail Sakhniuk <its545@yandex.ru>",
  "license": "MIT",
  "resolutions": {
    "@types/react": "18.0.0",
    "@types/react-dom": "18.0.0"
  },
  "devDependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.42",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "@types/react-virtualized": "^9.21.21",
    "@types/signals": "^1.0.1",
    "@types/webpack": "^5.28.0",
    "@types/webpack-env": "^1.17.0",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "concurrently": "^7.2.1",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "electron": "^19.0.4",
    "electron-builder": "^23.0.3",
    "electron-devtools-installer": "^3.2.0",
    "electron-is-dev": "^2.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^7.2.11",
    "html-webpack-plugin": "^5.5.0",
    "prettier": "^2.6.2",
    "react-refresh": "^0.13.0",
    "react-refresh-typescript": "^2.0.5",
    "rimraf": "^3.0.2",
    "sass-loader": "^13.0.0",
    "source-map-loader": "^3.0.1",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.3",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@material-ui/core": "^4.10.2",
    "@material-ui/icons": "^4.9.1",
    "axios": "^0.21.1",
    "classnames": "^2.2.6",
    "connected-react-router": "^6.8.0",
    "electron-log": "^4.2.2",
    "electron-window-state": "^5.0.3",
    "history": "^4.10.1",
    "lodash": "^4.17.19",
    "moment": "^2.27.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^6.3.0",
    "react-virtualized": "^9.22.3",
    "reconnecting-websocket": "^4.4.0",
    "redux": "^4.0.5",
    "redux-act": "^1.8.0",
    "redux-saga": "^1.1.3",
    "reflect-metadata": "^0.1.13",
    "reselect": "^4.0.0",
    "signals": "^1.0.0",
    "typedi": "^0.10.0"
  },
  "build": {
    "publish": {
      "provider": "github"
    },
    "productName": "Tradex App",
    "appId": "com.tradex.org",
    "files": [
      "package.json",
      "src/dist/",
      "src/charting_library/",
      "src/assets/"
    ],
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "mac": {
      "icon": "./resources/icon.png",
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "resources/ico-app.png",
      "signingHashAlgorithms": [
        "sha1",
        "sha256"
      ],
      "publisherName": [
        "tradex"
      ]
    },
    "nsis": {
      "oneClick": "true",
      "perMachine": "true"
    },
    "linux": {
      "target": [
        "deb",
        "rpm",
        "snap",
        "AppImage"
      ],
      "category": "Development"
    },
    "directories": {
      "buildResources": "assets",
      "output": "release"
    }
  },
  "homepage": "./"
}
